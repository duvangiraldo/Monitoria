
@{
    ViewBag.Title = "EncontrarAsesoria";
}
@model IEnumerable<ProyectoMonitores.Models.Asesoria>
<h2>EncontrarAsesoria</h2>

<table border="1">


    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Curso.nombreCurso)</td>
            <td>@Html.DisplayFor(modelItem => item.Monitor.nombreMonitor)</td>
            <td>@Html.DisplayFor(modelItem => item.fecha)</td>
            <td>@Html.DisplayFor(modelItem => item.aula)</td>
        </tr>

    }


</table>

